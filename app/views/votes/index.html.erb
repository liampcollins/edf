<h1>Listing votes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Finance</th>
    <th>Skills</th>
    <th>Env</th>
    <th>Culture</th>
    <th>Inf</th>
    <th>Diginf</th>
    <th>Agglom</th>
    <th>Market</th>
    <th>Lifestyle</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <td><%= vote.name %></td>
    <td><%= vote.title %></td>
    <td><%= vote.finance %></td>
    <td><%= vote.skills %></td>
    <td><%= vote.env %></td>
    <td><%= vote.culture %></td>
    <td><%= vote.inf %></td>
    <td><%= vote.diginf %></td>
    <td><%= vote.agglom %></td>
    <td><%= vote.market %></td>
    <td><%= vote.lifestyle %></td>
    <td><%= link_to 'Show', vote %></td>
    <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
    <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vote', new_vote_path %>
